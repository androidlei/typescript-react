{
  "name": "typescript-react",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean-dist && webpack -p --config=configs/webpack/prod.js",
    "clean-dist": "rm -f -r -d dist",
    "clean-lib": "rm -f -r -d lib",
    "lint": "npm run lint:ts && npm run lint:sass",
    "lint:ts": "tslint --fix './src/**/*.ts*' --format stylish --force",
    "lint:sass": "stylelint ./src/**/*.scss",
    "start": "npm run start-dev",
    "start-dev": "webpack-dev-server --config=configs/webpack/dev.js --open --inline",
    "start-pord": "npm run build && node index.js",
    "test": "jest --watch --coverage --config=configs/jest.json",
    "lib": "npm run clean-lib && webpack --config=configs/webpack/lib.js --progress --colors"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/androidlei/typescript-react.git"
